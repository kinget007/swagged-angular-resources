{
{{#each this}}
    {{> providerActionsDoc}}
    '{{camelize nickname}}': {
    method: '{{action}}',
    url: apiUrl + '{{path}}',
    {{#if pathParameters}}
        params: {
        {{#each pathParameters}}
            '{{name}}': '@{{name}}',
        {{/each}}
        },
    {{/if}}
    {{#isQuery}}
        isArray: true,
    {{/isQuery}}
    },
{{/each}}
}